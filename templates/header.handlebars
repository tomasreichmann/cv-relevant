<!DOCTYPE html>
{{#[helpers/headerData]}}
<html lang="{{lang}}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="copyright" content="{{copyright}}" >
    <meta name="description" content="{{description}}" >
    <style>
      body {
        display: none;
      }
    </style>
    <title>{{title}}</title>

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
    <!--navigation -->
    <!-- Static navbar -->
    <nav id="navigation" class="navbar navbar-default navbar-static-top yamm sticky" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">{{switchLabel}}</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="{{home.url}}" {{#home.active}}class="active"{{/home.active}}><img src="/assets/images/logo.gif" alt="{{home.title}}"></a>
        </div>
        <div class="navbar-collapse collapse">
          <!--ul class="nav navbar-nav navbar-right navbar-language">
            {{#otherLanguages}}
            <li><a href="{{url}}" >{{title}}</a></li>
            {{/otherLanguages}}
          </ul-->
          <ul class="nav navbar-nav navbar-right">
            {{#items}}
            <li {{#active}}class="active"{{/active}}><a href="{{url}}" >{{title}}</a></li>
            {{/items}}
          </ul>
        </div><!--/.nav-collapse -->
      </div><!--container-->
    </nav><!--navbar-default-->
{{/[helpers/headerData]}}
